<%- include('partials/header', { title: title }) %>



<div class="sticky-footer-wrapper">
    <div class="sticky-footer-content d-flex justify-content-center align-items-center flex-column">
        <div class="container text-center">
            <% if (error.status === 404) { %>
                <h1 class="display-4">404 Not Found</h1>
                <p class="lead mb-4">The page you are looking for might have been removed, had its name changed, or is temporarily unavailable.</p>
            <% } else { %>
                <h1 class="display-4">Error</h1>
                <h2 class="h3 mb-4"><%= message %></h2>
                <pre><%= error.stack %></pre>
            <% } %>
            <a href="/" class="btn btn-primary" style="background-color: rgb(188, 0, 0); border-color: rgb(165, 1, 1);">Go to Home Page</a>
        </div>
    </div>
</div>


<%- include('partials/footer') %>
